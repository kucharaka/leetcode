class Solution:
    def longestCommonPrefix(self, strs: List[str]) -> str:
        common_prefix = ""
        
        isCharEqual=True
        charQuantity = 0
        # Finding the shortest word in strs
        min_len = 201 
        min_index = 0
        for i in range(len(strs)):
            if len(strs[i]) < min_len:
                min_len = len(strs[i])
                min_index = i
        if min_len == 201:
            return ""
        
        if(len(strs)) == 1:
            return(strs[0])
        # The shortest word will be in beginning of strs 
        strs[0], strs[min_index] = strs[min_index], strs[0]
        
        # Comparing with shorest word
        for j in range(min_len):
            for i in range(1, len(strs)):
                if strs[0][j] == strs[i][j]:
                    continue
                else:
                    isCharEqual = False
                    break
            if isCharEqual:
                charQuantity+=1
                common_prefix = strs[0][:charQuantity]
                
        return common_prefix
                
