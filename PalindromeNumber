class Solution:
    def isPalindrome(self, x: int) -> bool:
        if x < 0:
            return False
        a = []
        while x > 0:
            a.append(x%10)
            x //= 10
           
        for i in range(len(a)):
            if a[i] == a[-i-1]:
                continue
            else:
                return False
        return True
                
