class Solution:
    def intToRoman(self, num: int) -> str:
        num_int = [1000, 900, 500, 400, 100, 90, 50, 40, 10, 9, 5, 4, 1]
        num_rom = ["M", "CM", "D", "CD", "C", "XC", "L", "XL", "X", "IX", "V", "IV", "I"]
        ans = ''
        i = 0
        while num:
            while num - num_int[i] >= 0:
                ans += num_rom[i]
                num -= num_int[i]
            i+=1
        return ans
                
